{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "target": "es6",
      "lib": ["es6", "dom"],
      "types": ["cypress"],
      "allowJs": true,
      "isolatedModules": false,
      "noEmit": true,
      "strict": true
    },
    "include": [
      "**/*.ts",
      "cypress/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }
  